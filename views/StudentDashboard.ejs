  <h4 style="max-width:100%;"><%= data.Name %> ( <%= data.Email %> )</h4>
  <address>
 <% if(data.InstituteName) { %>
   <%=  data.InstituteName %>, <br><b>Address: </b><%= data.InstituteAddress  %>, <%= data.InstituteCountry %>
